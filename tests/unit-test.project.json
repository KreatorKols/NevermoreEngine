{
  "name": "UnitTest",
  "globIgnorePaths": [ "**/.package-lock.json" ],
  "tree": {
    "$className": "DataModel",
    "ServerScriptService": {
      "UnitTest": {
        "$className": "Folder",
        "game": {
          "$path": "src/modules"
        },
        "node_modules": {
          "$path": "node_modules"
        },
        "jest.config": {
          "$path": "jest.config.lua"
        }
      },
      "UnitTestScripts": {
        "$path": "src/scripts/Server"
      }
    },
    "StarterPlayer": {
      "StarterPlayerScripts": {
        "UnitTestScripts": {
          "$path": "src/scripts/Client"
        }
      }
    }
  }
}